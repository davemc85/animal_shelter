

<h1>Animals</h1>

<% for animal in @animals %>

<p>Name: <%= animal.name() %> </p>
<p>type: <%= animal.type %> </p>
<p><img style="width: 100px"src="<%= animal.profile_pic %>" ></p>
<p>status: <%= animal.status %></p>
<p><a href="/animals/<%= animal.id %>">View Animal Profile</a></p>
<hr>

<% end %>
